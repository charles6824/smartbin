include includes/header.pug
block content
    link(rel='stylesheet' , href='/stylesheets/dashboard.css')
    link(rel="stylesheet" href="https://unpkg.com/leaflet@1.3.1/dist/leaflet.css" integrity="sha512-Rksm5RenBEKSKFjgI3a41vrjkw4EVPlJ3+OiI65vTjIdo9brlAacEuKOiQ5OFh7cOI1bkDwLqdLw3Zg0cRJAAQ==" crossorigin="")
    script(src="https://unpkg.com/leaflet@1.3.1/dist/leaflet.js" integrity="sha512-/Nsx9X4HebavoBvEBuyp3I7od5tA0UzAxs+j83KgC8PU0kgB4XiK4Lfe4y4cgBtaRJQEIFCW+oC506aPT2L1zw==" crossorigin="")
    script(async src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB0Li1yNhH_sq_woxAAww5Iwg4OVCIegDE&callback=initMap")
    body
        div(id="wrapper")
            include includes/sidenav.pug
            div(id="content-wrapper" class="d-flex flex-column")
                div(id="content")
                    include includes/usernav.pug
                    div(class="container")
                        .row
                            .col-md-4
                                form
                                    div(class="row")
                                        div(class="col-md-12 mb-3")
                                            label(class="label") Bin ID
                                            input(type="text" class="form-control" value=id readonly)
                                        div(class="col-md-12 mb-3")
                                            label(class="label") Bin Name
                                            input(type="text" class="form-control" value=name readonly)
                                        div(class="col-md-12 mb-3")
                                            label(class="label") Bin Level 
                                            input(type="text"  class="form-control" id="level" value=level readonly)
                                        div(class="col-md-12 mb-3")
                                            label(class="label") Bin Latitude 
                                            input(type="text" class="form-control" id="lat" value=lat readonly)
                                        div(class="col-md-12 mb-3")
                                            label(class="label") Bin Longitude
                                            input(type="text" class="form-control" id="lng" value=lng readonly)
                                
                            .col-md-8
                                div#map 
                               
        include includes/footer.pug
        //- script(src="/javascripts/bin.js" type="module")